{
  "name": "Links in Pocket",
  "description": "A extension that contains daily used all links one place. .",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/icon_16.png",
    "32": "/images/icon_48.png",
    "48": "/images/icon_96.png"
  }
}
